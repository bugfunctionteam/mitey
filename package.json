{
  "name": "auth-webapp",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "NODE_OPTIONS='--require ./src/utils/tracer.js' next start",
    "test": "jest",
    "test:frontend": "jest --selectProjects frontend",
    "test:backend": "jest --selectProjects backend",
    "test:coverage": "jest --config jest.coverage.config.js --coverage",
    "test:coverage:view": "open ./coverage/lcov-report/index.html",
    "test:watch": "jest --watch",
    "lint": "next lint --dir src",
    "lint:fix": "npm run lint -- --fix",
    "check-types": "tsc --pretty --noEmit",
    "prepare": "husky install",
    "husky:recommended": "rm -fr .husky && npm run prepare && npx husky add .husky/pre-commit 'npx lint-staged' && npx husky add .husky/post-commit 'git update-index --again' && npx husky add .husky/pre-push 'npm run check-types\nnpm test'",
    "codegen:swagger": "node codegen.swagger.js",
    "analyze": "ANALYZE=true next build",
    "syncDesign": "env FIGMA_TOKEN=7102-d2d3bd87-85ad-4985-a587-e5dd3568bdd8 figma-theme NR0Yv0VkIHQ65HY0uVwSvUWl --out-dir=./src/themes/figma"
  },
  "dependencies": {
    "@next-auth/mongodb-adapter": "^1.1.1",
    "axios": "^0.26.0",
    "cors": "^2.8.5",
    "dd-trace": "^3.2.0",
    "es-abstract": "^1.20.4",
    "formik": "^2.2.9",
    "mongodb": "^4.13.0",
    "next": "^12.3.1",
    "next-auth": "^4.18.7",
    "next-csrf": "^0.2.1",
    "nodemailer": "^6.8.0",
    "rc-progress": "^3.4.1",
    "react": "^18.0.0-0",
    "react-dom": "^18.2.0",
    "react-ssr-prepass": "^1.5.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.2.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/cors": "^2.8.12",
    "@types/node": "^18.0.4",
    "@types/nodemailer": "^6.4.7",
    "@types/react": "^18.0.15",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^26.3.0",
    "babel-plugin-styled-components": "^1.11.1",
    "casual": "^1.6.2",
    "cssnano": "^5.1.12",
    "eslint": "^8.22.0",
    "eslint-config-next": "^12.2.5",
    "eslint-import-resolver-typescript": "^3.4.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "figma-theme": "^1.0.0-3",
    "husky": "^8.0.1",
    "jest": "^27.1.3",
    "jest-environment-jsdom": "^27.1.3",
    "jest-sonar-reporter": "^2.0.0",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^13.0.3",
    "msw": "^0.47.0",
    "node-mocks-http": "^1.11.0",
    "postcss": "^8.4.14",
    "styled-components": "^5.3.5",
    "swagger-typescript-api": "^10.0.1",
    "typescript": "^4.3.1"
  }
}
